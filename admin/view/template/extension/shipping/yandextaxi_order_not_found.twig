{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <h1>{{ heading_title_order_not_found }}</h1>
            <ul class="breadcrumb">
                {% for breadcrumb in breadcrumbs %}
                    <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                {% endfor %}
            </ul>
        </div>
    </div>
    <div class="container-fluid">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><i class="fa fa-exclamation-triangle"></i> {{ heading_title_order_not_found }}</h3>
            </div>
            <div class="panel-body">
                <p class="text-center">{{ text_order_not_found }}</p>
            </div>
        </div>

        {{ include('extension/shipping/partial/_support_contact.twig') }}
    </div>
{{ footer }}
